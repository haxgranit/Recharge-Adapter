{
    "name": "recharge-platform-adapter",
    "version": "1.0.0",
    "private": true,
    "description": "",
    "scripts": {
        "build:version": "node scripts/build_version.js",
        "build": "cross-env NODE_ENV=production webpack --progress",
        "lint": "vue-cli-service lint",
        "lint:changed": "node scripts/lint_changed_files.js",
        "build-e2e": "cross-env NODE_ENV=end-to-end webpack --progress",
