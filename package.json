        "build-stage": "cross-env NODE_ENV=staging webpack --progress",
        "dev": "cross-env NODE_ENV=development webpack --watch --progress --color",
        "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales 'src/core/locales/**/*.json'",
        "local": "cross-env NODE_ENV=local webpack --watch --progress",
        "local-serve": "cross-env NODE_ENV=local webpack serve",
        "test": "vue-cli-service test:unit",
        "test-gh-actions": "vue-cli-service test:unit --reporters=default --reporters=jest-junit",
        "test:coverage": "vue-cli-service test:unit --coverage",
        "predev": "npm run build:version",
        "prebuild": "npm run build:version",
        "prebuild-e2e": "npm run build:version",
        "prebuild-dev": "npm run build:version",
        "prebuild-stage": "npm run build:version",
        "prelocal": "npm run build:version",
        "prelocal-serve": "npm run build:version"
    },
